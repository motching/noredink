package object noRedInk{

  def atoi(string:String, accu:Int):Int = {
    if (string.length != 0) {
      println(accu)
      println(string.head)
      atoi(string.tail, accu*10 + string.head.as)
    }
    accu
    }

  def main(args: Array[String]) :Unit={
    println(atoi("111", 0))
    println("111".toInt)
  }
}
