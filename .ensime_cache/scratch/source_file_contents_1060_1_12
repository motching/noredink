package object noRedInk{

  def atoi(string:String, accu:Int):Int = {
    println("ac"accu)
    if (string.length != 0) {
      println(string.head)
      atoi(string.tail, accu*10 + string.head.asDigit)
    }
    accu
    }

  def main(args: Array[String]) :Unit={
    println(atoi("1986", 0))
  }
}
